@model dt191g_projekt.Models.PortfolioModel
@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager

@if (SignInManager.IsSignedIn(User))
{
    <td class="crud-elements">
        <a asp-action="Create">Skapa en ny</a> |
        <a asp-action="Edit" asp-route-id="@Model.Id">Redigera</a> |
        <a asp-action="Details" asp-route-id="@Model.Id">Detaljer</a> |
        <a asp-action="Delete" asp-route-id="@Model.Id">Radera</a>
    </td>
}
else
{
    <td></td>
}